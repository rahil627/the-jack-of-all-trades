	public class Level extends Sprite {
		private var stageRef:Stage;
		private var blockHolder:Sprite;
		private var enemyHolder:Sprite;
		private var goalHolder:Sprite;
		private var turnAroundMarkerHolder:Sprite;
		private var levelNumber:int;
		private var temp:int = 0; //TODO
		
		public function Level(stageRef:Stage, levelNumber:int) {
			this.levelNumber = levelNumber;
			this.stageRef = stageRef;
			this.name = 'level';
			
			generate(levelNumber);
		}
		
		private function generate(levelNumber:int):void {						
			blockHolder = new Sprite(); //TODO: children vs dynamic array
			blockHolder.name = 'blockHolder'; //to use getChildByName
			this.addChild(blockHolder);
			
			enemyHolder = new Sprite();
			enemyHolder.name = 'enemyHolder';
			this.addChild(enemyHolder);
			
			goalHolder = new Sprite();
			goalHolder.name = 'goalHolder';
			this.addChild(goalHolder);
			
			turnAroundMarkerHolder = new Sprite();
			turnAroundMarkerHolder.name = 'turnAroundMarkerHolder';
			this.addChild(turnAroundMarkerHolder);
			
			var currentLevelArray:Array = getMap(levelNumber);
			
			//TODO: use 2d array for the levels!
			//var columns:int = (levelNumber == 4) ? Math.ceil(currentLevelArray.length/19) : Math.ceil(currentLevelArray.length/16); //16 rows
			var columns:int = Math.ceil(currentLevelArray.length/16); //16 rows
			var currentRow:int = 0;
			var blockWidth:int = Block.width;
			var blockHeight:int = Block.height;
			var block:Block;
			var enemy:Enemy;
			var goal:Goal;
			var turnAroundMarker:InvisibleMarker;
			var player:Player = stageRef.getChildByName('player') as Player;
			
			for (var i:int = 0; i < currentLevelArray.length; i++) {	
				//checking if we move onto the next row
				if(i % columns == 0)
					currentRow++;
				
				switch (currentLevelArray[i]) { //TODO: use char instead of int, for more map characters
					case 0:
						//do nothing
						break;
					case 1:
						block = new Block();
						block.x = (i-(currentRow-1)*columns)*blockWidth;
						block.y = (currentRow-1)*blockHeight;
						blockHolder.addChild(block);
						break;
					case 2:
						player.x = (i - (currentRow - 1) * columns) * blockWidth;
						player.y = (currentRow - 1) * blockHeight;
						break;
					case 3:
						enemy = new Dog(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						enemyHolder.addChild(enemy);
						break;
					case 4:
						goal = new Goal(this.stageRef);
						goal.x = (i - (currentRow - 1) * columns) * blockWidth;
						goal.y = (currentRow - 1) * blockHeight;
						goalHolder.addChild(goal);
						break;
					case 5:
						turnAroundMarker = new InvisibleMarker();
						turnAroundMarker.x = (i - (currentRow - 1) * columns) * blockWidth;
						turnAroundMarker.y = (currentRow - 1) * blockHeight;
						turnAroundMarkerHolder.addChild(turnAroundMarker);
						break;
					case 6:
						goal = new Goal(this.stageRef, true);
						goal.x = (i - (currentRow - 1) * columns) * blockWidth;
						goal.y = (currentRow - 1) * blockHeight;
						goalHolder.addChild(goal);
						break;
					case 7:
						enemy = new Professor(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						
						enemy.y -= 25; //TODO: this is awful
						
						enemyHolder.addChild(enemy);
						break;
						
					case 8:
						enemy = new BadInfluenceMan(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						
						enemy.y -= 25; //TODO: this is awful
						
						enemyHolder.addChild(enemy);
						break;
						
					case 9:
						enemy = new BusinessMan(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						
						enemy.y -= 25; //TODO: this is awful
						
						enemyHolder.addChild(enemy);
						break;
						
					case 10:
						enemy = new AngryBusinessMan(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						
						enemy.y -= 25; //TODO: this is awful
						
						enemyHolder.addChild(enemy);
						break;
						
					case 11:
						goal = new Goal(this.stageRef, false, true);
						goal.x = (i - (currentRow - 1) * columns) * blockWidth;
						goal.y = (currentRow - 1) * blockHeight;
						goalHolder.addChild(goal);
						break;
						
					case 12:
						enemy = new Magnate(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						
						enemy.y -= 25; //TODO: this is awful
						
						enemyHolder.addChild(enemy);
						break;
					
					case 13:
						enemy = new Doppelganger(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						enemyHolder.addChild(enemy);
						break;
						
					case 14:
						enemy = new Mother(this.stageRef);
						enemy.x = (i - (currentRow - 1) * columns) * blockWidth;
						enemy.y = (currentRow - 1) * blockHeight;
						
						enemy.y -= 25;
						
						enemyHolder.addChild(enemy);
						break;
						
					default:
						trace('invalid level character');
				}	
			}
			
			//center the screen on player
			var distanceToCenter:int = 250 - player.x;
			this.x += distanceToCenter
			player.x += distanceToCenter;
			
			//this.y -= 100;
			//player.y -= 100;
		}
